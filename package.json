{
  "name": "nextjs-mobx-keystone",
  "version": "1.0.0",
  "repository": "https//github.com/EvgeniyKumachev/nextjs-mobx-keystone-template.git",
  "author": "Evgeniy Kumachev <evgeniykumachev@protonmail.ch>",
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache --ext .ts,.tsx \"{src,types}/**/*\" --fix",
    "lint:icons": "eslint --cache --ext .ts,.tsx \"src/components/icons/**/*\" --fix",
    "generate-icons": "rimraf src/components/icons && svgr --config-file ./config/svgr/config.js --typescript --ignore-existing -d src/components/icons assets/icons && yarn lint:icons"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run generate-icons && lint-staged",
      "post-merge": "yarn run generate-icons"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn eslint --ext .ts,.tsx --fix"
    ]
  },
  "dependencies": {
    "@emotion/babel-plugin": "11.0.0-next.13",
    "@emotion/cache": "11.0.0-next.13",
    "@emotion/css": "^11.0.0-next.13",
    "@emotion/react": "^11.0.0-next.13",
    "@emotion/server": "^11.0.0-next.12",
    "@emotion/styled": "^11.0.0-next.12",
    "emotion-theming": "^11.0.0-next.6",
    "mobx": "^5.15.4",
    "mobx-keystone": "^0.45.0",
    "mobx-react-lite": "2.0.7",
    "mobx-utils": "^5.6.1",
    "next": "9.4.5-canary.45",
    "next-compose-plugins": "^2.2.0",
    "next-seo": "^4.7.3",
    "nookies": "^2.3.2",
    "path-to-regexp": "^6.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@svgr/cli": "^5.3.1",
    "@types/cookie": "^0.4.0",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.6",
    "@types/signale": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "cross-env": "^7.0.2",
    "csstype": "^2.6.10",
    "dotenv": "^8.2.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.4",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.0",
    "signale": "^1.4.0",
    "ts-toolbelt": "^6.15.0",
    "typescript": "^3.9.7"
  },
  "keywords": [],
  "description": ""
}